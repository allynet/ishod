{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": [
    "typedoc-plugin-markdown"
  ],
  "entryPoints": [
    "./README.md",
    "./src/result.ts"
  ],
  "router": "structure-dir",
  "outputs": [
    {
      "name": "html",
      "path": "docs/html"
    },
    {
      "name": "json",
      "path": "docs/docs.json"
    },
    {
      "name": "markdown",
      "path": "docs/md"
    }
  ],
  "excludeInternal": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "exclude": [
    "node_modules",
    "dist",
    "docs"
  ]
}